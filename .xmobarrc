Config { font = "-xos4-terminus-medium-r-*-*-13-*-*-*-*-*-*-*"
, lowerOnStart = True
, bgColor = "black"
, fgColor = "grey"
, position = Top
, commands =
    [ Run Cpu ["-L", "3", "-H", "50", "--low", "#51a351", "--normal", "#51a351", "--high", "#bd362f"] 10
    , Run Memory ["-t", "Mem: <usedratio>%"] 10
    , Run Date "%s %b %_d %H:%M:%S" "date" 10
    , Run Network "eth0" ["-L", "0", "-H", "32", "--low", "#51a351", "--normal", "#51a351", "--high", "#bd362f"] 10
    , Run StdinReader
    ]
, sepChar = "%"
, alignSep = "}{"
, template = "%StdinReader% }{%cpu% <fc=#555555>|</fc> %memory% <fc=#555555>|</fc> %eth0% <fc=#555555>|</fc> <fc=#5bc0de>%date%</fc>"
