Config { font = "-xos4-terminus-medium-r-*-*-13-*-*-*-*-*-*-*"
, lowerOnStart = True
, bgColor = "#2b2b2b"
, fgColor = "#666666"
, position = Bottom
, commands =
    [ Run Cpu ["-L", "3", "-H", "50", "--low", "#fad46b", "--normal", "#90aede", "--high", "#eb3652"] 10
    , Run Memory ["-t", "Mem: <usedratio>%", "-L", "1024", "-H", "4096", "--low", "#fad46b", "--normal", "#90aede", "--high", "#eb3652"] 10
    , Run Date "%a %d %b %H:%M:%S" "date" 10
    , Run Network "eth0" ["-L", "0", "-H", "32", "--low", "#fad46b", "--normal", "#90aede", "--high", "#eb3652"] 10
    , Run StdinReader
    ]
, sepChar = "%"
, alignSep = "}{"
, template = " %StdinReader% }{%cpu% <fc=#555555>|</fc> %memory% <fc=#555555>|</fc> %eth0% <fc=#555555>|</fc> <fc=#90aede>%date%</fc> "
